{
    "name": "mauriziocingolani/yii2-fmwk-php",
    "description": "Parte php del framework basato su Yii2",
    "keywords": [
        "yii2",
        "framework",
        "php"
    ],
    "authors": [
        {
            "name": "Maurizio Cingolani",
            "email": "mauriziocingolani74@gmail.com",
            "homepage": "http://www.mauriziocingolani.it"
        }
    ],
    "type": "library",
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "2.0.26"
    },
    "autoload": {
        "psr-4": {
            "mauriziocingolani\\yii2fmwkphp\\": "/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    }
}
